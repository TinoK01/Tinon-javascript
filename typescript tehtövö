  [code: number]: string;
}
const codeToStates: Code = {603: 'NH',
617: 'MA'};

interface ReverseCode {
  [code: string]: number;
}
const stateToCodes: ReverseCode = {'NH':
603, 'MA': 617};

interface Profile {
  name: string;
  age: number,
  hobbies?: string[];
}

// The property, hobbies, is optional, but name and age are required.
const teacher: Profile = {name: 'Tom Sawyer', age: 18};